page {
  box-sizing: border-box;
  position: relative;
  min-height: 100%;
  line-height: 1;
  background: #fff;
}
input {
  height: auto;
}
button.authorize {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  border: none;
  border-radius: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}
.placeholder {
  color: #999;
}
