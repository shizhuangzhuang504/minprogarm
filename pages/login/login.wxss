.header-wapper {
  /* height: 250rpx; */
  width: 100%;
  border-bottom: 10rpx solid #EDEDED;
}
.img-content {
  width: 330rpx;
  height: 120rpx;
  margin: 65rpx auto;
}
.img-content .img-icon {
  width: 100%;
  height: 100%;
}
.login-title {
  font-size: 24rpx;
  text-align: center;
  font-weight: 700;
  padding: 30rpx 0;
  letter-spacing: 4rpx;
}
.intro-info {
  padding-left: 220rpx;
}
.intro-info .item {
  padding: 15rpx 0;
  position: relative;
  font-size: 30rpx;
}
.intro-info .item::before{
  content: '';
  position: absolute;
  left: -30rpx;
  top: 20rpx;
  width: 14rpx;
  height: 14rpx;
  border-radius: 14rpx;
  background: #F7511E;
}
.btn-wapper {
  width: 100%;
  text-align: center;
  margin-top: 60rpx;
}
.btn-wapper .authorize-info {
  width: 550rpx;
  height: 90rpx;
  background: #F7511E;
  color: #fff;
  text-align: center;
  line-height: 90rpx;
  margin-bottom: 40rpx;
  font-size: 36rpx;
}
.give-btn {
  width: 550rpx;
  height: 90rpx;
  background: #F3F3F3;
  color: #9F9F9F;
  text-align: center;
  line-height: 90rpx;
  font-size: 36rpx;
}

.free-wrap{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1000;
  background: rgba(0, 0, 0, .25);
  display: flex;
  justify-content: center;
  align-items: center;
}
.free{
  width:722rpx;
  height:766rpx;
  position:relative;
}
.free image{
  width:100%;
  height:100%;
  display:block;
}
.date{
  position:absolute;
  font-size: 25rpx;
	color: #fff;
  left:90rpx;
  bottom:65rpx;
}
.cancel{
  padding:0 5rpx;
  font-size: 42rpx;
	color: #fff;
  position:absolute;
  right:28rpx;
  top:5rpx;
}
.calc-car{
  position: fixed;
  left: 13rpx;
  bottom: 20rpx;
  z-index: 999;
}

.free-coupon{
  position: relative;
  display:flex;
  flex-direction:column;
  margin:0  auto;
  text-align: center;
  justify-content:center;
  align-items: center;
}
.free-info{
  margin:0 auto;
  width:590rpx;
  position: relative;
}
.auth-info {
  margin:0 auto;
  width:550rpx;
  height:410rpx;
  background: #fff;
  border-radius: 10rpx;
}
.logo-icon {
  width: 110rpx;
  height: 100rpx;
  margin: 30rpx auto;
}
.logo-icon .img-icon {
  width: 110rpx;
  height: 100rpx;
}
.phone-title {
  text-align: center;
  font-size: 30rpx;
  color: #333333;
  margin-bottom: 30rpx;
}
.phone-info {
  text-align: center;
  font-size: 30rpx;
  color: #535353;
  margin-bottom: 52rpx;
}
.agree-btn {
  display: flex;
  border-top: 1rpx solid #E5E5E5;
}
.agree-btn .refuse {
  flex: 1;
  border-right: 1rpx solid #E5E5E5;
  box-sizing: border-box;
  font-size: 36rpx;
  line-height: 106rpx;
  text-align: center;
  color: #999999;
}
.agree-btn .agree {
  flex: 1;
  font-size: 36rpx;
  line-height: 106rpx;
  text-align: center;
  color: #333333;
  background: #fff;
  border: none;
  padding: 0;
}
.agree-btn .agree::after {
  border:none;
}